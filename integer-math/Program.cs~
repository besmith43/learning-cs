using System;

namespace integer_math
{
    class Program
    {
        static void Main(string[] args)
        {
		int a = 18;
		int b = 6;
		int c = a + b;
		Console.WriteLine(c);

		c = a - b;
		Console.WriteLine(c);

		c = a * b;
		Console.WriteLine(c);

		c = a / b;
		Console.WriteLine(c);

		a = 5;
		b = 4;
		c = 2;
		d = a + b * c;
		Console.WriteLine(d);

		a = 5;
		b = 4;
		c = 2;
		d = (a + b) * c;
		Console.WriteLine(d);

		d = (a + b) - 6 * c + (12 * 4) / 3 + 12;
		Console.WriteLine(d);

		a = 7;
		b = 4;
		c = 3;
		d = (a  + b) / c;
		Console.WriteLine(d);

		a = 7;
		b = 4;
		c = 3;
		d = (a  + b) / c;
		int e = (a + b) % c;
		Console.WriteLine($"quotient: {d}");
		Console.WriteLine($"remainder: {e}");

		int max = int.MaxValue;
		int min = int.MinValue;
		Console.WriteLine($"The range of integers is {min} to {max}");

		int what = max + 3;
		Console.WriteLine($"An example of overflow: {what}");

		double w = 5;
		double x = 4;
		double y = 2;
		double z = (w + x) / y;
		Console.WriteLine(z);

		w = 19;
		x = 23;
		y = 8;
		z = (w + x) / y;
		Console.WriteLine(z);

		double maxi = double.MaxValue;
		double mini = double.MinValue;
		Console.WriteLine($"The range of double is {mini} to {maxi}");

		double third = 1.0 / 3.0;
		Console.WriteLine(third);

		decimal miniu = decimal.MinValue;
		decimal maxiu = decimal.MaxValue;
		Console.WriteLine($"The range of the decimal type is {miniu} to {maxiu}");

		x = 1.0;
		y = 3.0;
		Console.WriteLine(x / y);

		decimal s = 1.0M;
		decimal t = 3.0M;
		Console.WriteLine(s / t);

		double radius = 2.50;
		double area = Math.PI * radius * radius;
		Console.WriteLine(area);
        }
    }
}
